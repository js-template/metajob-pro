{
  "kind": "collectionType",
  "collectionName": "private_pages",
  "info": {
    "singularName": "private-page",
    "pluralName": "private-pages",
    "displayName": "Private Page",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "role": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users",
      "configurable": false
    },
    "title": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "blocks": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "dynamiczone",
      "components": [
        "block.category-card",
        "block.blog-card",
        "metajob-block.bookmark",
        "metajob-block.applied-jobs",
        "metajob-block.manage-resume",
        "widget.total-job",
        "widget.open-job",
        "widget.menu-widget",
        "widget.matched-list",
        "widget.favorite-list",
        "widget.count-card",
        "widget.copyright-bar",
        "widget.contact-widget",
        "widget.closed-job",
        "widget.applied-list",
        "metajob-block.manage-company"
      ]
    },
    "styles": {
      "displayName": "grid container",
      "type": "component",
      "repeatable": false,
      "component": "component.grid-container"
    },
    "seo": {
      "type": "component",
      "repeatable": false,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "shared.seo"
    }
  }
}
