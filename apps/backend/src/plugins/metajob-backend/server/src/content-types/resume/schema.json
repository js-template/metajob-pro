{
  "kind": "collectionType",
  "collectionName": "resumes",
  "info": {
    "singularName": "resume",
    "pluralName": "resumes",
    "displayName": "Resume",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "default": "Candidate Name"
    },
    "slug": {
      "type": "uid",
      "targetField": "name",
      "required": true
    },
    "tagline": {
      "type": "text",
      "required": true,
      "default": "Professional Title"
    },
    "category": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::metajob-backend.job-category",
      "required": true
    },
    "user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user",
      "required": true
    },
    "skills": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "plugin::metajob-backend.skill"
    },
    "experience_time": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::metajob-backend.experience-level"
    },
    "salary": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::metajob-backend.avg-salary"
    },
    "salary_type": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::metajob-backend.salary-type"
    },
    "contact": {
      "type": "component",
      "repeatable": false,
      "component": "metajob-block.contact"
    },
    "about": {
      "type": "richtext",
      "default": "Candidate about"
    },
    "language": {
      "type": "enumeration",
      "enum": [
        "Arabic",
        "Bengali",
        "Bulgarian",
        "Catalan",
        "Chinese (Simplified)",
        "Chinese (Traditional)",
        "Croatian",
        "Czech",
        "Danish",
        "Dutch",
        "English",
        "Estonian",
        "Filipino",
        "Finnish",
        "French",
        "Galician",
        "Georgian",
        "German",
        "Greek",
        "Gujarati",
        "Hebrew",
        "Hindi",
        "Hungarian",
        "Icelandic",
        "Indonesian",
        "Irish",
        "Italian",
        "Japanese",
        "Javanese",
        "Kannada",
        "Kazakh",
        "Korean",
        "Kurdish (Kurmanji)",
        "Kyrgyz",
        "Lao",
        "Latin",
        "Latvian",
        "Lithuanian",
        "Macedonian",
        "Malay",
        "Malayalam",
        "Maltese",
        "Maori",
        "Marathi",
        "Mongolian",
        "Nepali",
        "Norwegian",
        "Pashto",
        "Persian",
        "Polish",
        "Portuguese",
        "Punjabi",
        "Romanian",
        "Russian",
        "Serbian",
        "Slovak",
        "Slovenian",
        "Somali",
        "Spanish",
        "Sundanese",
        "Swahili",
        "Swedish",
        "Tamil",
        "Telugu",
        "Thai",
        "Turkish",
        "Ukrainian",
        "Urdu",
        "Uzbek",
        "Vietnamese",
        "Welsh",
        "Xhosa",
        "Yiddish",
        "Yoruba",
        "Zulu"
      ],
      "default": "English"
    },
    "show_profile": {
      "type": "enumeration",
      "enum": ["Show", "Hide"],
      "default": "Show"
    },
    "education": {
      "type": "component",
      "repeatable": true,
      "component": "metajob-block.experience"
    },
    "experience": {
      "type": "component",
      "repeatable": true,
      "component": "metajob-block.experience"
    },
    "portfolio": {
      "type": "component",
      "repeatable": true,
      "component": "metajob-block.portfolio"
    },
    "seo": {
      "type": "component",
      "repeatable": false,
      "component": "shared.seo"
    }
  }
}
