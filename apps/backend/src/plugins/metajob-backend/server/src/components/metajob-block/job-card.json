{
  "info": {
    "displayName": "Job Card",
    "icon": "collapse",
    "description": "Job card block"
  },
  "category": "metajob-block",
  "modelName": "job-card",
  "uid": "metajob-block.job-card",
  "pluginOptions": {},
  "attributes": {
    "content": {
      "type": "component",
      "repeatable": false,
      "component": "config.section-title"
    },
    "currency": {
      "type": "string",
      "default": "USD"
    },
    "vacancy_placeholder": {
      "type": "string",
      "default": "Vacancy"
    },
    "card_button": {
      "type": "component",
      "repeatable": false,
      "component": "config.link"
    },
    "button": {
      "type": "component",
      "repeatable": false,
      "component": "config.link"
    },
    "item_count": {
      "type": "integer",
      "default": 6,
      "min": 1,
      "max": 18
    },
    "empty": {
      "type": "component",
      "repeatable": false,
      "component": "shared.empty"
    },
    "style": {
      "type": "component",
      "repeatable": false,
      "component": "config.style-section"
    }
  }
}
