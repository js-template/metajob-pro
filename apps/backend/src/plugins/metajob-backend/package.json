{
  "name": "@jstemplate/metajob-backend",
  "version": "0.0.4-alpha.2",
  "private": true,
  "description": "Metajob Backend",
  "license": "MIT",
  "keywords": [],
  "homepage": "https://github.com/js-template/metajob",
  "bugs": {
    "url": "https://github.com/js-template/metajob/issues/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/js-template/metajob"
  },
  "author": "https://github.com/js-template/metajob",
  "type": "commonjs",
  "exports": {
    "./package.json": "./package.json",
    "./strapi-admin": {
      "types": "./dist/admin/src/index.d.ts",
      "source": "./admin/src/index.ts",
      "import": "./dist/admin/index.mjs",
      "require": "./dist/admin/index.js",
      "default": "./dist/admin/index.js"
    },
    "./strapi-server": {
      "types": "./dist/server/src/index.d.ts",
      "source": "./server/src/index.ts",
      "import": "./dist/server/index.mjs",
      "require": "./dist/server/index.js",
      "default": "./dist/server/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "strapi-plugin build",
    "watch": "strapi-plugin watch",
    "watch:link": "strapi-plugin watch:link",
    "verify": "strapi-plugin verify",
    "test:ts:front": "run -T tsc -p admin/tsconfig.json",
    "test:ts:back": "run -T tsc -p server/tsconfig.json",
    "publish:pack": "npm pack --pack-destination dist",
    "publish:upload": "node scripts/publish.js",
    "publish": "npm run publish:pack && npm run publish:upload"
  },
  "dependencies": {
    "@strapi/design-system": "^2.0.0-rc.12",
    "@strapi/icons": "^2.0.0-rc.12",
    "react-intl": "^6.8.7"
  },
  "devDependencies": {
    "@strapi/sdk-plugin": "^5.2.7",
    "@strapi/strapi": "^5.4.0",
    "@strapi/typescript-utils": "^5.4.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "dashify": "^2.0.0",
    "node-fetch": "^2.6.1",
    "prettier": "^3.3.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.28.0",
    "styled-components": "^6.1.13",
    "typescript": "^5.6.3"
  },
  "peerDependencies": {
    "@strapi/sdk-plugin": "^5.2.7",
    "@strapi/strapi": "^5.4.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.28.0",
    "styled-components": "^6.1.13"
  },
  "strapi": {
    "kind": "plugin",
    "name": "metajob-backend",
    "displayName": "Metajob Backend",
    "description": ""
  }
}
