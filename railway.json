{
  "$schema": "https://railway.app/railway.schema.json",
  "build": {
    "builder": "NIXPACKS",
    "nixpacksPlan": {
      "phases": {
        "yarn:config": {
          "dependsOn": ["setup"],
          "cmds": [
            "yarn config set @padma:registry https://api.keygen.sh/v1/accounts/88de7a21-b541-48e5-8727-f992ebeb43fa/artifacts",
            "yarn config set //api.keygen.sh/v1/accounts/88de7a21-b541-48e5-8727-f992ebeb43fa/artifacts/:_authToken $LICENSE_TOKEN"
          ]
        },
        "install": {
          "dependsOn": ["setup", "yarn:config"],
          "cmds": ["yarn install"]
        }
      }
    }
  }
}
