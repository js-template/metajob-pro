{
  "$schema": "https://railway.app/railway.schema.json",
  "build": {
    "builder": "NIXPACKS",
    "nixpacksPlan": {
      "phases": {
        "npm:config": {
          "dependsOn": ["setup"]
        },
        "install": {
          "dependsOn": ["setup", "npm:config"]
        }
      }
    }
  }
}
